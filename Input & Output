#Hello World
print("HELLO WORLD!")

#Hello World with tab
print("HELLO WORLD\t HELLO WORLD")

#Hello World with new line
print("HELLO WORLD\n HELLO WORLD")

#Integer Input
a=int(input())
print(a)

#Float Input
a=float(input())
print("%.2f"%a)

#Character Input
a=(input())
print(a)

#String Input
a=str(input())
print(a)

#Ascii-1
a=(input())
print(ord(a))

#Ascii-2
a=int(input())
print(chr(a))
